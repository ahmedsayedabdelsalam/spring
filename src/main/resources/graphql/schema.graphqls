type Book{
    id: ID!
    name: String
    author: Author!
}
type Author {
  id: ID!
  name: String
  age: Int
  books: [Book]
}


type Query {
   index: [Author]
   show(id: Int): Author
}

input AuthorDto{
    name: String!
    age: Int!
}

type Mutation {
    create(authDto: AuthorDto): Author
}